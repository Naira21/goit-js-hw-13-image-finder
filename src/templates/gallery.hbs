{{#each this}}
<li class='gallery-item'>
  <div class='photo-card'>
    <div class='img-wrap'>
      <img src='{{this.webformatURL}}' alt='' data-src='{{this.largeImageURL}}' />
    </div>

    <div class='stats'>
      <p class='stats-item'>
        <i class='material-icons'>thumb_up</i>
        {{this.likes}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>visibility</i>
        {{this.views}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>comment</i>
        {{this.comments}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>cloud_download</i>
        {{this.downloads}}
      </p>
    </div>
  </div>
</li>
{{/each}}